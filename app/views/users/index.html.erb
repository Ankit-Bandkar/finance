
<table class="table">
<thead>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Date</th>
  </tr>
</thead>

<tbody>
    <% @user.each do |f| %>
        <tr>
        <td><%= f.name %></td>
        <td><%= f.email %></td>
        <td><%= f.created_at.strftime("%F") %></td>
        </tr>
        <% end %>
</tbody>
</table>    