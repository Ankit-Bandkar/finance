<%= form_for @organization, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <h3>Contact No</h3>
  <div id='Contact'>
    <%= f.fields_for :contacts do |contact| %>
      <%= render 'contact_fields', :f => contact %>
    <% end %>
    <div class='links'>
      <%= link_to_add_association 'add contact', f, :contacts %>
    </div>
  </div>

  <div>
    <%= f.submit class:"btn btn-success" %>
  </div>  
<% end %>